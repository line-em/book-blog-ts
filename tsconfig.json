{
	"extends": "astro/tsconfigs/strict",
	"compilerOptions": {
		"jsx": "react-jsx",
		"jsxImportSource": "react",
		"target": "ESNext",
		"strictNullChecks": true,
		"module": "ESNext",
		"plugins": [
			{
				"name": "@astrojs/ts-plugin"
			}
		],
		"baseUrl": ".",
		"paths": {
			"@/*": [
				"./src/*"
			]
		}
	}
}
